{
    "name": "beddel",
    "version": "1.0.8",
    "description": "Declarative Sequential Pipeline Executor for YAML workflows with native streaming and Vercel AI SDK v6 support",
    "author": "Bota na Rede",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/botanarede/beddel.git"
    },
    "homepage": "https://beddel.com.br",
    "bugs": {
        "url": "https://github.com/botanarede/beddel/issues"
    },
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./server": {
            "import": "./dist/server.js",
            "types": "./dist/server.d.ts"
        },
        "./client": {
            "import": "./dist/client.js",
            "types": "./dist/client.d.ts"
        }
    },
    "scripts": {
        "build": "tsc && npm run copy-agents",
        "copy-agents": "rsync -av --include='*/' --include='*.yaml' --exclude='*' src/agents/ dist/agents/",
        "dev": "tsc --watch"
    },
    "dependencies": {
        "ai": "^6.0.41",
        "@ai-sdk/google": "^3.0.10",
        "@ai-sdk/amazon-bedrock": "^4.0.19",
        "@openrouter/ai-sdk-provider": "^2.0.0",
        "@modelcontextprotocol/sdk": "^1.25.2",
        "zod": "^4.3.5",
        "js-yaml": "^4.1.0"
    },
    "devDependencies": {
        "typescript": "^5.6.3",
        "@types/node": "^25.0.9",
        "@types/js-yaml": "^4.0.9"
    }
}
