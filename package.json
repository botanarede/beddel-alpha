{
    "name": "beddel",
    "version": "1.0.4",
    "description": "Declarative Sequential Pipeline Executor for YAML workflows with native streaming and Vercel AI SDK v6 support",
    "author": "Bota na Rede",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/botanarede/beddel.git"
    },
    "homepage": "https://beddel.com.br",
    "bugs": {
        "url": "https://github.com/botanarede/beddel/issues"
    },
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./server": {
            "import": "./dist/server.js",
            "types": "./dist/server.d.ts"
        },
        "./client": {
            "import": "./dist/client.js",
            "types": "./dist/client.d.ts"
        }
    },
    "scripts": {
        "build": "tsc && npm run copy-agents",
        "copy-agents": "cp -r src/agents/*.yaml dist/agents/",
        "dev": "tsc --watch",
        "test": "vitest"
    },
    "dependencies": {
        "@ai-sdk/amazon-bedrock": "^4.0.3",
        "@ai-sdk/google": "^3.0.1",
        "@openrouter/ai-sdk-provider": "^1.5.4",
        "ai": "^6.0.3",
        "js-yaml": "^4.1.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^22.10.2",
        "typescript": "^5.6.3",
        "vitest": "^4.0.16"
    }
}
